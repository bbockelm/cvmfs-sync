
universe = local

environment = XRD_LOGLEVEL=Debug PYTHONUNBUFFERED=1
executable = ligo-data-update
use_x509userproxy = true
output = ligo_update_logs/lastrun.out
error = ligo_update_logs/lastrun.err
log = ligo_update_logs/log

cron_month = *
cron_day_of_month = *
cron_day_of_week = *
cron_hour = *
cron_minute = 14

on_exit_remove = false

queue

